# InsightEngine Environment Variables

# Required: Google API key for Gemini
GOOGLE_API_KEY=your_gemini_api_key_here

# Optional: Embedding settings
# Set to "true" to use deterministic dummy embeddings instead of real ones
# Useful for development without using API quota
# USE_DUMMY_EMBEDDINGS=false

# Optional: Debugging options
# VERBOSE_EMBEDDINGS=false
# DEBUG_EMBEDDINGS=false
# DEBUG_QUALITY_ASSESSMENT=false
# DEBUG_EVALUATION=false
# DEBUG_INSIGHTS=false

# Optional: Database path override
# DB_PATH=memory.db

# Memory Quality Assessment Settings
# QUALITY_THRESHOLD=0.6
# USE_LLM_QUALITY_EVALUATION=false
# MAX_FILTERED_MEMORIES=10
# MAX_FEEDBACK_HISTORY=10

# Quality dimension weights (must sum to 1.0)
# QUALITY_WEIGHT_INFO_DENSITY=0.25
# QUALITY_WEIGHT_COHERENCE=0.25
# QUALITY_WEIGHT_SPECIFICITY=0.25
# QUALITY_WEIGHT_FACTUALITY=0.25

# Memory filtering weights
# QUALITY_SIMILARITY_WEIGHT=0.7
# QUALITY_SCORE_WEIGHT=0.3

# Self-Reflection System Settings
# ASYNC_EVALUATION=true
# EVALUATION_FREQUENCY=1.0
# DEFAULT_INSIGHT_CONFIDENCE=0.7
# PERFORM_META_EVALUATION=false
# USE_EMBEDDINGS_FOR_INSIGHTS=true
# AUTO_CONSOLIDATE_INSIGHTS=true
# INSIGHT_CONSOLIDATION_THRESHOLD=0.85